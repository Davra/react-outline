{
  "name": "react-outline",
  "version": "1.3.1",
  "description": "react styling framework to keep your makeup clean",
  "main": "./dist/index.js",
  "scripts": {
    "build": "babel ./index.js --presets babel-preset-es2015 --plugins transform-object-rest-spread --out-dir dist",
    "test": "jest --no-cache ",
    "test:update": "npm test -- --updateSnapshot",
    "test:cover": "npm test -- --coverage --no-cache",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codemeasandwich/react-outline.git"
  },
  "keywords": [
    "react",
    "css",
    "css-in-js",
    "react styling",
    "styled-components",
    "prefixer",
    "inline styles",
    "autoprefixer",
    "vendor prefix"
  ],
  "author": "Brian Shannon (http://www.codemeasandwich.com)",
  "license": "Apache-2.0 ",
  "bugs": {
    "url": "https://github.com/codemeasandwich/react-outline/issues"
  },
  "homepage": "https://github.com/codemeasandwich/react-outline#readme",
  "dependencies": {
    "add-px-to-style": "^1.0.0",
    "hyphenate-style-name": "^1.0.2",
    "inline-style-prefixer": "^3.0.6",
    "react": "^15.6.1"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "jest": "^20.0.4",
    "raw-loader": "^0.5.1",
    "react-test-renderer": "^15.6.1"
  },
  "jest": {
    "globals": {
      "__TEST__": true
    },
    "moduleNameMapper": {
      "\\.(css|jpg|png)$": "<rootDir>/empty-module.js",
      "^raw-loader": "<rootDir>/empty-module.js",
      "react-outline": "<rootDir>/index.js",

      "^react-addons-css-transition-group$": "<rootDir>/empty-module.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverageFrom": ["index.js"],
    "verbose": true
  }
}
